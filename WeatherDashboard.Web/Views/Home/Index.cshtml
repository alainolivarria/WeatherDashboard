


<div class="col-sm-12 col-sm-offset-1 col-md-12 col-md-offset-0 main">
    <h1 class="page-header">Dashboard</h1>

    <div class="row placeholders">
        <div class="col-xs-8 col-sm-9 placeholder">
            <div id="tempGraph" style="min-width: 310px; height: 400px; margin: 0 auto"></div>
        </div>
        <div class="col-xs-4 col-sm-3 placeholder">
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">City</label>
                <div class="col-sm-10">
                    <select id="selCity" class="form-control">
                    </select>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">Scale</label>
                <div class="col-sm-10">
                    <select id="selScale" class="form-control">
                    </select>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">Date</label>
                <div class="col-sm-10">
                    <div id="datepicker1"></div>
                </div>
            </div>
        </div>
    </div>

    <h2 class="sub-header">List of temperatures</h2>
    <div class="table-responsive">
        <table id="gridTemps" class="display table table-striped" cellspacing="0" >
            <thead>
                <tr>
                    <th style="width: auto">Date</th>
                    <th style="width: auto">Temperature</th>

                </tr>
            </thead>
        </table>

    </div>
</div>



